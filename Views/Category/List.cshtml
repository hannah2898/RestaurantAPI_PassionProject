@model IEnumerable<RestaurantAPI_PassionProject.Models.CategoryDto>
@{
    ViewBag.Title = "List";
}

<h2>Categories</h2>
<div><a href="/Category/New">Add Category</a></div>

@foreach (var category in Model)
{
    <h2>@category.CategoryName</h2>
    <div><a style="color:#ab2020;" href="/Category/DeleteConfirm/@category.CategoryId">Delete Category</a></div>


}

